{
  "name": "web-api",
  "version": "1.0.0",
  "description": "HTTP API for temperature sensor data",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite-node src/index.ts",
    "build": "node ./scripts/generate-version.cjs && tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "check-formatting": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "fix-formatting": "prettier --write \"src/**/*.{ts,js,json,md}\""
  },
  "author": "Ethan Lemke",
  "license": "MIT",
  "dependencies": {
    "express": "5.1.0",
    "openapi3-ts": "4.4.0",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@types/express": "5.0.1",
    "@types/node": "22.15.18",
    "@types/swagger-ui-express": "4.1.8",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite-node": "3.1.3"
  }
}
